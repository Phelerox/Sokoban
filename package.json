    "@babel/runtime": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "humanize-string": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "polka": "^0.5.2",
    "postcss-cli": "^6.1.2",
    "rollup": "^1.15.6",
